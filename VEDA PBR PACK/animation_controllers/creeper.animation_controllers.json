{
	"format_version" : "1.10.0",
	"animation_controllers" : {
	 "controller.animation.creeper.nyengir" : {
	 "initial_state": "default",
   "states": {
    "default": {
     "transitions": [
      {
       "hwehe": "query.swell_amount>0  && q.anim_time>0"
      }
     ]
    },
    "malah_menjauh": {
     "sound_effects": [
      {
       "effect": "kamu_gitu"
      }
    ],
     "transitions": [
      {
       "hwehe": "query.swell_amount>0"
      }
     ]
    },
    "hwehe": {
      "sound_effects": [
      {
       "effect": "hwehehehe"
      }
    ],
     "transitions": [
      {
       "malah_menjauh": "!query.swell_amount>0"
      }
     ]
    }
   }
  },
  "controller.animation.creeper.head" : {
			"initial_state" : "default",
			"states" : {
				"default" : {
					"animations" : [ "creeper_head" ]
				}
			}
		},
		"controller.animation.creeper.legs" : {
			"initial_state" : "default",
			"states" : {
				"default" : {
					"animations" : [ "creeper_legs" ]
				}
			}
		},
		"controller.animation.creeper.swelling" : {
			"initial_state" : "default",
			"states" : {
				"default" : {
					"animations" : [ "creeper_swelling" ]
				}
			}
		}
 } 
}
