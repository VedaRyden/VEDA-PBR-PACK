{
 "world_modal_progress_panel": {
  "controls": [
   {
    "base_content": {
     "anchor_from": "top_left",
     "anchor_to": "top_left",
     "controls": [
      {
       "progress_title_text@progress.progress_title_text": {
        "anchor_from": "bottom_middle",
        "anchor_to": "bottom_middle",
        "offset": [
         0,
         15
        ]
       }
      },
      {
       "progress_bar_text@progress.progress_bar_text": {
        "localize": false,
        "max_size": [
         "100% - 8px",
         40
        ],
        "offset": [
         0,
         -10
        ],
        "size": [
         "default",
         40
        ]
       }
      }
     ],
     "offset": [
      6,
      50
     ],
     "size": [
      "100%-12px",
      "100%-12px"
     ],
     "type": "panel"
    }
   },
   {
    "inside_content": {
     "anchor_from": "bottom_middle",
     "anchor_to": "bottom_middle",
     "controls": [
      {
       "loading_bar_panel": {
        "controls": [
         {
          "fancy_progress_loading_bars@progress.fancy_progress_loading_bars": {}
         },
         {
          "progress_loading_bars@progress.progress_loading_bars": {
           "offset": [
            0,
            100
           ]
          }
         }
        ],
        "offset": [
         0,
         100
        ],
        "size": [
         "100%",
         "100%"
        ],
        "type": "panel"
       }
      }
     ],
     "size": [
      "100%-12px",
      "100%"
     ],
     "type": "stack_panel"
    }
   }
  ]
 }
}