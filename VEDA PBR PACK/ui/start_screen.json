{
  "namespace": "start",
  "copyright": {
    "type": "panel",
    "controls": [
      {
        "label": {
          "type": "label",
          "color": "$main_header_text_color",
          "layer": 2,
          "text": "Modded Version, Copyright @Mojang AB",
          "size": [ "default", 10 ],
          "max_size": [ "100%", "100%" ],
          "anchor_from": "top_left",
          "anchor_to": "top_left"
        }
      },
      {
        "label_background": {
          "type": "image",
          "texture": "textures/ui/Black",
          "anchor_from": "top_left",
          "anchor_to": "top_left",
          "offset": [ -1, -1 ],
          "alpha": 0.6,
          "size": [ "100%sm + 4px", "100%sm + 2px" ],
          "layer": 1
        }
      }
    ]
  },
  "version": {
    "type": "panel",
    "anchor_from": "top_right",
    "anchor_to": "top_right",
    "size": [ "50%", "100%" ],
    "controls": [
      {
        "label": {
          "type": "label",
          "color": "$main_header_text_color",
          "layer": 2,
          "text": "Raiden Deffered v1.2/              ",
          "size": [ "default", 10 ],
          "max_size": [ "100%", "100%" ],
          "anchor_from": "top_right",
          "anchor_to": "top_right",
          "bindings": [
            {
              "binding_name": "#version"
            }
          ]
        }
      },
      {
        "label1": {
          "type": "label",
          "color": "$main_header_text_color",
          "layer": 3,
          "text": "#version",
          "size": [ "default", 10 ],
          "max_size": [ "100%", "100%" ],
          "anchor_from": "top_right",
          "anchor_to": "top_right",
          "bindings": [
            {
              "binding_name": "#version"
            }
          ]
        }
      },
      {
        "label_background": {
          "type": "image",
          "texture": "textures/ui/Black",
          "anchor_from": "top_right",
          "anchor_to": "top_right",
          "offset": [ 1, -1 ],
          "alpha": 0.6,
          "size": [ "100%sm + 2px", "100%sm + 2px" ],
          "layer": 1
        }
      }
    ]
  },
  "text_panel": {
    "type": "panel",
    "size": [ "100%-2px", 10 ],
    "offset": [ 0, -1 ],
    "anchor_from": "bottom_middle",
    "anchor_to": "bottom_middle",
    "controls": [
      {
        "copyright@start.copyright": {}
      },
      {
        "development_version@start.development_version": {
          "ignored": "$is_publish"
        }
      },
      {
        "version@start.version": {}
      }
    ]
  }
}
